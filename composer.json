{
  "name": "lessname/queue",
  "description": "Queue your work",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "php": "^8.1.0",

    "lessname/value-object": "^0.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^9.5.0",
    "vimeo/psalm": "^4.10.0",

    "php-amqplib/php-amqplib": "^3.2",
    "lessname/database": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "LessQueue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessQueueTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
